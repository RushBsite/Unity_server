namespace Protocol;

union fbsId{
	S_EnterGame,
	S_LeaveGame,
	S_Spawn,
	C_Move,
	S_Move,
}

struct Vec3{
	x:float;
	y:float;
	z:float;
}

struct PlayerInfo{
	player_id:int32;
	name:[ubyte:32];
	pos:Vec3;
}


table S_EnterGame{
	player:PlayerInfo;
}

table S_LeaveGame{
}

table S_Spawn{
	players:[PlayerInfo];
}

table S_Desapwn{
	playerIds:[int32];
}

table C_Move{
	pos:Vec3;
}

table S_Move{
	player_id:int32;
	pos:Vec3;
}

